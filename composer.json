{
  "name": "truegameover/php-keyboard-input-correction",
  "description": "Correction of input from the keyboard. Allows validating & correcting wrong input from the keyboard's layout. E.g. writing something on ru layout when en is active.",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "php",
    "input",
    "correction"
  ],
  "homepage": "https://github.com/TrueGameover/php-keyboard-input-correction",
  "authors": [
    {
      "name": "TrueGameover",
      "email": "truegameover@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "KeyboardInputCorrection\\": "src/"
    }
  }
}
